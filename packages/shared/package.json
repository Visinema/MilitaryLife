{
  "name": "@mls/shared",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./constants": {
      "types": "./dist/constants.d.ts",
      "default": "./dist/constants.js"
    },
    "./api-types": {
      "types": "./dist/api-types.d.ts",
      "default": "./dist/api-types.js"
    },
    "./game-types": {
      "types": "./dist/game-types.d.ts",
      "default": "./dist/game-types.js"
    },
    "./npc-registry": {
      "types": "./dist/npc-registry.d.ts",
      "default": "./dist/npc-registry.js"
    }
  },
  "scripts": {
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "build": "tsc -p tsconfig.json",
    "lint": "echo 'shared: no lint configured'"
  },
  "devDependencies": {
    "typescript": "^5.7.2"
  }
}

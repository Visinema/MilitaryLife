{
  "name": "@mls/web",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "next dev -p 3000",
    "build": "node ./scripts/check-version-unique.mjs && node ./scripts/check-utf8-sources.mjs && node ./scripts/next-build.mjs",
    "start": "next start -p 3000",
    "check:version": "node ./scripts/check-version-unique.mjs",
    "check:utf8": "node ./scripts/check-utf8-sources.mjs",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "lint": "next lint"
  },
  "dependencies": {
    "@mls/shared": "workspace:*",
    "next": "15.5.10",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "zustand": "^5.0.2"
  },
  "devDependencies": {
    "@next/swc-wasm-nodejs": "15.5.7",
    "@types/node": "^22.10.2",
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.3.5",
    "autoprefixer": "^10.4.20",
    "eslint": "8.57.1",
    "eslint-config-next": "15.5.10",
    "postcss": "^8.4.49",
    "tailwindcss": "^3.4.16",
    "typescript": "^5.7.2"
  }
}
